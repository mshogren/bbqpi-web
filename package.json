{
  "name": "bbqpi-web",
  "version": "1.0.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mshogren/bbqpi-web.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/git": "^9.0.0",
    "babel-preset-airbnb": "^4.4.0",
    "codecov": "^3.6.5",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-flowtype": "^5.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "react-scripts": "^3.4.1",
    "semantic-release": "^17.0.4",
    "source-map-explorer": "^2.4.1"
  },
  "dependencies": {
    "@attently/riek": "^2.0.1",
    "bootstrap": "^4.4.1",
    "firebase": "^7.12.0",
    "prop-types": "^15.7.2",
    "rc-slider": "^9.2.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-charts": "^3.0.15",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.11.0",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=node --coverage && codecov",
    "analyze": "source-map-explorer build/static/js/main.*",
    "jest": "react-scripts test --env=node",
    "posttest": "eslint src",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release",
    "update": "rm -rf node_modules && rm package-lock.json && ncu -u && npm install"
  },
  "babel": {
    "presets": [
      "airbnb"
    ]
  },
  "eslintConfig": {
    "extends": "./.eslintrc.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "npmPublish": false,
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
